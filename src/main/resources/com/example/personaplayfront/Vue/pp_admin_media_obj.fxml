<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox fx:id="container" alignment="CENTER" prefHeight="50.0" prefWidth="1080.0" style="-fx-background-color: #202020;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.personaplayfront.Controller.Frontend.PpAdminMediaObjController">
   <children>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="180.0">
         <children>
            <Text fx:id="title" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="title" textAlignment="CENTER">
               <font>
                  <Font size="12.0" />
               </font>
            </Text>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="180.0">
         <children>
            <Text fx:id="imdb_id" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="imdb_id" textAlignment="CENTER">
               <font>
                  <Font name="System" size="12.0" />
               </font>
            </Text>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="180.0">
         <children>
            <Text fx:id="year" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="year" textAlignment="CENTER">
               <font>
                  <Font name="System" size="12.0" />
               </font>
            </Text>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="180.0">
         <children>
            <CheckBox fx:id="visibleToggle" mnemonicParsing="false" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="180.0">
         <children>
            <CheckBox fx:id="availableToggle" mnemonicParsing="false" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="180.0">
         <children>
            <Button fx:id="locationButton" mnemonicParsing="false" text="Location" />
         </children>
      </VBox>
   </children>
</HBox>
