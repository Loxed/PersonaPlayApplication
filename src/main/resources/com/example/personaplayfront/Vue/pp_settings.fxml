                          <ImageView fitHeight="28.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                  <Image url="@../Icon/white_bookmarks-outline.png" />
                              </image>
                              <HBox.margin>
                                  <Insets right="10.0" />
                              </HBox.margin>
                          </ImageView>
                          <Text fx:id="subsButton" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Subscriptions" textAlignment="CENTER">
