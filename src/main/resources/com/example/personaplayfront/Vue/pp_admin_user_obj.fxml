<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox fx:id="container" alignment="CENTER" prefHeight="50.0" prefWidth="1080.0" style="-fx-background-color: #202020;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.personaplayfront.Controller.Frontend.PpAdminUserObjController">
   <children>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="180.0">
         <children>
            <Circle fx:id="userIcon" radius="20.0" stroke="TRANSPARENT" strokeType="INSIDE" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="180.0">
         <children>
            <Text fx:id="username" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="username" textAlignment="CENTER">
               <font>
                  <Font size="12.0" />
               </font>
            </Text>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="180.0">
         <children>
            <Text fx:id="email" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="email" textAlignment="CENTER">
               <font>
                  <Font name="System" size="12.0" />
               </font>
            </Text>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="180.0">
         <children>
            <Text fx:id="password" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="password" textAlignment="CENTER">
               <font>
                  <Font name="System" size="12.0" />
               </font>
            </Text>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="180.0">
         <children>
            <CheckBox fx:id="roleToggle" mnemonicParsing="false" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="180.0">
         <children>
            <CheckBox fx:id="matureToggle" mnemonicParsing="false" />
         </children>
      </VBox>
   </children>
</HBox>
